  "builds": [{ "src": "packages/web/build/**/*.*", "use": "@now/static" }],
  "routes": [
    { "src": "/", "dest": "/packages/web/build/index.html" },
    { "src": "/(?<path>.*)", "dest": "/packages/web/build/$path" }
